	var KEYCODE_ENTER = 13;
	var KEYCODE_ESC = 27;


function llamadaAjaxPrograma(event){
	
	if(event.keyCode == KEYCODE_ENTER || event.keyCode == KEYCODE_ESC) {		
	  document.getElementById('suggest-macrobuscador').style.display='none';
    }else{
	
		var caja = document.getElementById("texto-libre");
		var codigo = caja.value;
		
		if(!codigo.includes('select') 
		 && !codigo.includes('insert')
		 && !codigo.includes('update')
		 && !codigo.includes('truncate')
		 && !codigo.includes('drop')
		 && !codigo.includes('where')
		 && !codigo.includes('and')
		 && !codigo.includes('delete')
		 && !codigo.includes('from')
		 && !codigo.includes('or')
		 && !codigo.includes('script')
		 && !codigo.includes('alert')
		 && !codigo.includes('telnet')
		 && !codigo.includes('nslookup')
		 && !codigo.includes('ping')
		 && !codigo.includes('delay')
		 && !codigo.includes('<')
		 && !codigo.includes('>')){
		
		codigo = codigo.replace ("\u00D1","|");
		codigo = codigo.replace ("\u00f1","|");
		codigo = codigo.replace ("+","mas");		
				
			var sugProg=document.getElementById("suggest-macrobuscador");	
			sugProg.style.display='none';			
			if(codigo.length>=3){						
				var urlCargarNombresProg = '/cs/ContentServer?pagename=DeustoR/Suggest/INTR_nombreProgramasSuggest';			
				urlCargarNombresProg += '&texto-libre='+codigo+'&hizkuntza='+hizkuntza+'&SITE='+SITE;			
				 $.ajax({
				  type: "GET",
				  url: urlCargarNombresProg,				  	  
				  async: true,
				  success: function(data){				
					if(data!=null && data!=""){
						sugProg.innerHTML=data;
						sugProg.style.display='block';	
					}else{
						sugProg.innerHTML='';
						sugProg.style.display='none';
					}
				  }
				 });
			}	
		 }//fin if
	}
}
</script><script type="text/javascript">/*jQuery(document).ready(function(){$(".contents").find("table").each(function(i){if($(this).find("th").length){var selectedElement = $(this).find("th");var esTablaHorizontal=($(this).find("th").attr("scope")=="row");if(!esTablaHorizontal){jQuery($(this)).magicTable();}}}); });*/		</script></body></html>