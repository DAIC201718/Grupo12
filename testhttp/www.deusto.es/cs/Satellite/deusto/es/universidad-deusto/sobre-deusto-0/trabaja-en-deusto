		function setCookie(NameOfCookie, value, expiredays, secure){
			var ExpireDate = new Date ();
			ExpireDate.setTime(ExpireDate.getTime() + (expiredays * 24 * 3600 * 1000));
			document.cookie = NameOfCookie + "=" + escape(value) + ";domain=.deusto.es;path=/"+
			((expiredays == null) 
				? "" 
				: "; expires=" + ExpireDate.toGMTString()) + ((secure == true) ? "; secure" : "");
		}
