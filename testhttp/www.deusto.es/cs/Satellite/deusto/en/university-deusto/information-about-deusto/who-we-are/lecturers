	var selPrograma = "Select a degree";
	var SITE = 'Deusto';
	var strLocalizador='3000527900;3000528066;1340042318705;1340042365525;';
	var strIdContenido='1340042365525';

	function ordenarProfesores(orden){
		
		var a_inves = document.getElementById('apellidos').value;
		var n_inves = document.getElementById('nombre').value;
		var f_inves = document.getElementById('clmFacultad').value;
		var tipoEstudioG = document.getElementById('rdgrado').checked;		
		var tipoEstudioP = document.getElementById('rdpostgrado').checked;
		var doble_tit = document.getElementById('doble-tit').value;
		
				
		var urlobtProfesores = '/cs/ContentServer?pagename=DeustoR/Comun/Deusto_gestionEmpleados';			
		urlobtProfesores += '&accion=op&orden='+orden+'&hizkuntza='+hizkuntza+'&SITE='+SITE+'&localizador='+strLocalizador+'&cid='+strIdContenido;			
		
		if(n_inves!=''){
			urlobtProfesores += '&nombre='+n_inves;			
		}
		if(a_inves!=''){
			urlobtProfesores += '&apellidos='+a_inves;			
		}
		if(f_inves!='' && f_inves!='0'){
			urlobtProfesores += '&facultad='+f_inves;			
		}
		
		if(tipoEstudioG==true){
			urlobtProfesores += '&tipoEstudio=0';			
		}else if (tipoEstudioP==true){
			urlobtProfesores += '&tipoEstudio=1';			
		}
		
		if(doble_tit!=''){
			urlobtProfesores += '&doble_tit='+doble_tit;			
		}
		//alert("urlobtProfesores:"+urlobtProfesores);
		$.ajax({
		  type: "GET",
		  url: urlobtProfesores,				  	  
		  async: false,
		  success: function(data){	
			$("#buscador_ivestigadores").html(data);
			}
		});
	}

</script><div class="search-big-box form expand"><div class="search-content form bannerProfesor"><form action="/cs/Satellite" method="post" class="formBuscador"><input type="hidden" value="A" name="orden"/><input type="hidden" value="Deusto/Page/Deusto_Profesores_Template" name="pagename"/><input type="hidden" value="Page" name="c"/><input type="hidden" value="1340042365525" name="cid"/><input type="hidden" value="3000527900;3000528066;1340042318705;1340042365525;" name="localizador"/><input type="hidden" value="en" name="hizkuntza"/><input type="hidden" value="si" name="buscar"/><input type="hidden" value="" name="doble-tit" id="doble-tit"/><fieldset><div class="campo"><div class="elements col-md-4 col-xs-12"><label class="col-md-12 col-xs-4">Surname</label><input name="apellidos" class="col-md-12 text-box" id="apellidos" type="text" value="" /></div><div class="elements col-md-4 col-xs-12"><label class="col-md-12 col-xs-12">Type of study</label><input type="radio" value="0" checked="true" name="tipoEstudio" id="rdgrado"> <label class="mr-25 tipoEstudio">BA degrees</label><input type="radio" value="1"  name="tipoEstudio" id="rdpostgrado"> <label class="tipoEstudio">Postgraduate degrees</label></div></div><div class="campo"><div class="elements col-md-4 col-xs-12"><label class="col-md-12 col-xs-3">Name</label><input name="nombre" class="col-md-12 text-box" id="nombre" type="text" value="" /></div><div class="elements col-md-4 col-xs-12"><label class="col-md-12 col-xs-4">Faculty</label><div class="select-container"><select name="clmFacultad" class="text-box" id="clmFacultad"><option value='' selected >Choose a Faculty</option><option selected value="0">All faculties</option><option  value="600">Law</option><option  value="601">Psychology and Education</option><option  value="602">DBS</option><option  value="603">Sociales y Humanas</option><option  value="604">Engineering</option><option  value="605">Theology</option></select><div class="overlay-arrow"></div></div></d